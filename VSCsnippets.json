	// func setStylesByClassName
	"func setStylesByClassName": {
		"prefix": "fss",
		"scope": "javascript",
		"body": [
			"function setStylesByClassName(classNameOf, styles) {",
			"    const className = document.querySelectorAll(`.\\${classNameOf}`);",
			"    className.forEach(elem => {",
			"        elem.style.cssText += styles;",
			"    });",
			"}",
			"setStylesByClassName(\"$1\", `",
			"    $0",
			"`);",
		],
		"description": "func setStylesByClassName"
	},


	// function renderBlockByClassName
	"function renderBlockByClassName": {
		"prefix": "frbс",
		"scope": "javascript",
		"body": [
			"function renderBlockByClassName (classNamePosition, position, htmlBlock) {",
			"    const className = document.querySelector(`.\\${classNamePosition}`);",
			"    className.insertAdjacentHTML(position, htmlBlock);",
			"}",
			"",
			"renderBlockByClassName (",
			"    '${1:where-is}',",
			"    '${2:afterend}',",
			"    `",
			"    $0",
			"    `",
			");",
		],
		"description": "function renderBlockByClassName"
	},


	// floating-elements
	"floating-elements": {
		"prefix": "fe",
		"scope": "html",
		"body": [
			"<!-- _____ BEGIN: floating elements _____ -->",
			"<div class=\"floating-elements\"></div>",
			"<!-- _____ END: floating elements _____ -->",
			"<script>",
			"   /* -------- BEGIN: floating elements -------- */",
			"   const floatingElements = document.querySelectorAll('.floating-elements');",
			"",
			"   function getRandomInt(min, max) {",
			"      return Math.floor(Math.random() * (max - min)) + min;",
			"   }",
			"   function moving (nodeList, speed) {",
			"      window.addEventListener('mousemove', (e) => {",
			"         let x = Math.floor((e.pageX)/speed - 20) + \"px\";",
			"         let y = Math.floor((e.pageY)/speed) + \"px\";",
			"         nodeList.style.transform = 'translate('+x+' , '+y+')';",
			"      });",
			"   }",
			"   function moveAll (nodeList) {",
			"      // ъ-- Чем больше значение скорости, тем медленнее --ъ",
			"      // 1. Вариант с детальной настройкой скорости",
			"      moving(nodeList[0], 60);",
			"      moving(nodeList[1], 40);",
			"      moving(nodeList[2], 60);",
			"      moving(nodeList[3], 60);",
			"      moving(nodeList[4], 60);",
			"      moving(nodeList[5], 40);",
			"      moving(nodeList[6], 60);",
			"   ",
			"   ",
			"      // 2. Вариант с рандомной настройкой скорости",
			"      // nodeList.forEach(elem => {",
			"      //    moving(elem, getRandomInt(40, 60));",
			"      // });",
			"   }",
			"   moveAll(floatingElements);",
			"   /* -------- END: floating elements -------- */",
			"</script>"
		],
		"description": "floating-elements"
	},
